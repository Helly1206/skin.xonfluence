<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol>1</defaultcontrol>
    <!-- This is the original Confluence popup seek bar, so only show it if that setting is active -->
	<visible>[!Skin.HasSetting(HidePopupSeekBar) + !Skin.HasSetting(MinimumPopUpSeekBar)] + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding | Player.ShowTime | Player.IsTempo | !String.IsEmpty(Player.SeekNumeric)]</visible>
	<visible>!Player.HasGame</visible>
    <visible>!Player.FrameAdvance</visible>
	<animation effect="fade" start="0" end="100" time="150">WindowOpen</animation>
    <animation effect="fade" start="100" end="0" time="150">WindowClose</animation>
    <depth>DepthOSD</depth>
    <controls>
        <control type="group">
            <visible>player.chaptercount + Window.IsVisible(FullScreenVideo)</visible>
            <animation effect="slide" start="0,0" end="-150,0" time="0" condition="system.getbool(input.enablemouse)">Conditional</animation>
            <animation effect="slide" start="0,0" end="-60,0" time="75" condition="Player.Muted">conditional</animation>
            <left>1258r</left>
            <top>-9</top>
            <include>VisibleFadeEffect</include>
            <control type="image">
                <left>0</left>
                <top>0</top>
                <width>425</width>
                <height>105</height>
                <colordiffuse>EEFFFFFF</colordiffuse>
                <texture border="12">OverlayDialogBackground.png</texture>
            </control>
            <control type="label" id="1">
                <description>Chapter Pos No</description>
                <left>30</left>
                <top>15</top>
                <width>365</width>
                <height>30</height>
                <align>center</align>
                <aligny>center</aligny>
                <font>font12_title</font>
                <textcolor>grey2</textcolor>
                <label>[COLOR=$VAR[MainColorOne]]$LOCALIZE[21396][/COLOR] $INFO[player.chapter]$INFO[player.chaptercount, $LOCALIZE[31088] ]</label>
                <visible>player.chaptercount</visible>
            </control>
            <control type="label" id="1">
                <description>Chapter Name</description>
                <left>30</left>
                <top>45</top>
                <width>365</width>
                <height>30</height>
                <align>center</align>
                <aligny>center</aligny>
                <font>font10_title</font>
                <textcolor>grey2</textcolor>
                <label>$INFO[Player.ChapterName]</label>
                <visible>player.chaptercount</visible>
            </control>
        </control>
        <control type="group">
<!--            <animation effect="slide" start="0,0" end="292,998" time="0" condition="Skin.HasSetting(HidePopUpSeekBar)">Conditional</animation>
            <animation effect="slide" start="0,0" end="-150,0" time="0" condition="!Skin.HasSetting(HidePopUpSeekBar) + system.getbool(input.enablemouse)">Conditional</animation>
            <animation effect="slide" start="0,0" end="-60,0" time="75" condition="!Skin.HasSetting(HidePopUpSeekBar) + Player.Muted">conditional</animation> -->
            <left>940r</left>
            <top>-9</top>
            <visible>!Window.IsVisible(busydialog)</visible>
            <control type="image">
                <left>0</left>
                <top>0</top>
                <width>555</width>
                <height>105</height>
                <colordiffuse>EEFFFFFF</colordiffuse>
                <texture border="12">OverlayDialogBackground.png</texture>
            </control>
            <control type="group">
                <left>390</left>
                <top>15</top>
                <control type="image">
                    <left>15</left>
                    <top>0</top>
                    <width>120</width>
                    <height>75</height>
                    <texture>$VAR[OSDPath]OSDSeekFrame.png</texture>
                </control>
                <control type="image">
                    <left>0</left>
                    <top>4</top>
                    <width>30</width>
                    <height>66</height>
                    <texture>$VAR[OSDPath]OSDSeekRewind.png</texture>
                    <visible>Player.Rewinding | [Player.IsTempo + String.StartsWith(Player.PlaySpeed,0.)]</visible>
                </control>
                <control type="image">
                    <left>120</left>
                    <top>4</top>
                    <width>30</width>
                    <height>66</height>
                    <texture>$VAR[OSDPath]OSDSeekForward.png</texture>
                    <visible>Player.Forwarding | [Player.IsTempo + String.StartsWith(Player.PlaySpeed,1.)]</visible>
                </control>
                <control type="image">
                    <left>46</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSDPause.png</texture>
                    <visible>Player.Paused</visible>
                </control>
                <control type="image">
                    <left>46</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSDPlay.png</texture>
                    <visible>Player.Playing + !Player.IsTempo</visible>
                </control>
                <control type="image">
                    <left>42</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD2x.png</texture>
                    <visible>Player.Rewinding2x</visible>
                </control>
                <control type="image">
                    <left>42</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD0.9x.png</texture>
                    <visible>String.IsEqual(Player.PlaySpeed,0.90)</visible>
                </control>
                <control type="image">
                    <left>38</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD4x.png</texture>
                    <visible>Player.Rewinding4x</visible>
                </control>
                <control type="image">
                    <left>38</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD0.8x.png</texture>
                    <visible>String.IsEqual(Player.PlaySpeed,0.80)</visible>
                </control>
                <control type="image">
                    <left>33</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD8x.png</texture>
                    <visible>Player.Rewinding8x</visible>
                </control>
                <control type="image">
                    <left>28</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD16x.png</texture>
                    <visible>Player.Rewinding16x</visible>
                </control>
                <control type="image">
                    <left>26</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD32x.png</texture>
                    <visible>Player.Rewinding32x</visible>
                </control>
                <control type="image">
                    <left>51</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD2x.png</texture>
                    <visible>Player.Forwarding2x</visible>
                </control>
                <control type="image">
                    <left>51</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD1.1x.png</texture>
                    <visible>String.IsEqual(Player.PlaySpeed,1.10)</visible>
                </control>
                <control type="image">
                    <left>56</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD4x.png</texture>
                    <visible>Player.Forwarding4x</visible>
                </control>
                <control type="image">
                    <left>56</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD1.2x.png</texture>
                    <visible>String.IsEqual(Player.PlaySpeed,1.20)</visible>
                </control>
                <control type="image">
                    <left>60</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD8x.png</texture>
                    <visible>Player.Forwarding8x</visible>
                </control>
                <control type="image">
                    <left>60</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD1.3x.png</texture>
                    <visible>String.IsEqual(Player.PlaySpeed,1.30)</visible>
                </control>
                <control type="image">
                    <left>64</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD16x.png</texture>
                    <visible>Player.Forwarding16x</visible>
                </control>
                <control type="image">
                    <left>64</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD1.4x.png</texture>
                    <visible>String.IsEqual(Player.PlaySpeed,1.40)</visible>
                </control>
                <control type="image">
                    <left>68</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD32x.png</texture>
                    <visible>Player.Forwarding32x</visible>
                </control>
                <control type="image">
                    <left>68</left>
                    <top>6</top>
                    <width>60</width>
                    <height>60</height>
                    <texture>$VAR[OSDPath]OSD1.5x.png</texture>
                    <visible>String.IsEqual(Player.PlaySpeed,1.50)</visible>
                </control>
            </control>
            <control type="label">
                <description>Seekbar Label</description>
                <left>30</left>
                <top>10</top>
                <width>360</width>
                <height>30</height>
                <align>left</align>
                <aligny>center</aligny>
                <font>font10_title</font>
                <textcolor>$VAR[MainColorOne]</textcolor>
                <label>$VAR[SeekLabel]</label>
            </control>
            <control type="label" id="1">
                <description>Seek Number label</description>
                <left>560</left>
                <top>34</top>
                <width>195</width>
                <height>30</height>
                <align>left</align>
                <aligny>center</aligny>
                <font>font13_title</font>
                <textcolor>white</textcolor>
                <shadowcolor>black</shadowcolor>
                <label>$INFO[Player.SeekNumeric(hh:mm:ss)]</label>
            </control>
            <control type="group">
			    <visible>![Window.IsVisible(VideoOSD) | Window.IsVisible(MusicOSD)]</visible>
				<visible>!Player.ShowInfo</visible>
				<visible>[!Skin.HasSetting(ShowSeekBarPaused) + Player.Paused] | !Player.Paused</visible>
				<control type="group">
                    <visible>[VideoPlayer.Content(LiveTV) + VideoPlayer.HasEpg]</visible>
				    <control type="label">
                        <description>Elapsed Time Label</description>
                        <left>30</left>
                        <top>34</top>
                        <width>360</width>
                        <height>30</height>
                        <font>font13_title</font>
                        <textcolor>white</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[PVR.EpgEventElapsedTime] - $INFO[PVR.EpgEventDuration]</label>
                        <visible>!Player.Seeking</visible>
                    </control>
                    <control type="label">
                        <description>Seek Time Label</description>
                        <left>30</left>
                        <top>34</top>
                        <width>360</width>
                        <height>30</height>
                        <font>font13_title</font>
                        <textcolor>white</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[PVR.EpgEventSeekTime] - $INFO[PVR.EpgEventDuration]</label>
                        <visible>Player.Seeking</visible>
                    </control>
                    <control type="progress">
                        <description>ProgressbarTimeshift</description>
                        <left>30</left>
                        <top>68</top>
                        <width>360</width>
                        <height>22</height>
                        <info>PVR.TimeshiftProgressBufferEnd </info>
                        <midtexture border="6,0,6,0">OSDProgressMidLight.png</midtexture>
                        <visible>PVR.IsTimeShift</visible>
                    </control>
                    <control type="progress" id="23">
                        <description>Progressbar</description>
                        <left>30</left>
                        <top>68</top>
                        <width>360</width>
                        <height>22</height>
                        <info>PVR.EpgEventProgress</info>
                        <visible>true</visible>
                    </control>
                    <control type="slider" id="403">
                        <description>Seek Slider</description>
                        <left>30</left>
                        <top>63</top>
                        <width>360</width>
                        <height>18</height>
                        <texturesliderbar>seekslider.png</texturesliderbar>
                        <textureslidernib>osd_slider_nib.png</textureslidernib>
                        <textureslidernibfocus>osd_slider_nib.png</textureslidernibfocus>
                        <visible>Player.Seeking</visible>
                    </control>
                </control>
                <control type="group">
                    <visible>![VideoPlayer.Content(LiveTV) + VideoPlayer.HasEpg]</visible>
				    <control type="label">
                        <description>Elapsed Time Label</description>
                        <left>30</left>
                        <top>34</top>
                        <width>360</width>
                        <height>30</height>
                        <font>font13_title</font>
                        <textcolor>white</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[Player.Time] - $INFO[Player.Duration]</label>
                        <visible>!Player.Seeking</visible>
                    </control>
                    <control type="label">
                        <description>Seek Time Label</description>
                        <left>30</left>
                        <top>34</top>
                        <width>360</width>
                        <height>30</height>
                        <font>font13_title</font>
                        <textcolor>white</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[Player.SeekTime] - $INFO[Player.Duration]</label>
                        <visible>Player.Seeking</visible>
                    </control>
                    <control type="progress">
                        <description>ProgressbarCache</description>
                        <left>30</left>
                        <top>68</top>
                        <width>360</width>
                        <height>22</height>
                        <info>Player.ProgressCache</info>
                        <midtexture border="6,0,6,0">OSDProgressMidLight.png</midtexture>
                        <visible>true</visible>
                    </control>
                    <control type="progress" id="23">
                        <description>Progressbar</description>
                        <left>30</left>
                        <top>68</top>
                        <width>360</width>
                        <height>22</height>
                        <info>Player.Progress</info>
                        <visible>true</visible>
                    </control>
                    <control type="slider" id="401">
                        <description>Seek Slider</description>
                        <left>30</left>
                        <top>63</top>
                        <width>360</width>
                        <height>18</height>
                        <texturesliderbar>seekslider.png</texturesliderbar>
                        <textureslidernib>osd_slider_nib.png</textureslidernib>
                        <textureslidernibfocus>osd_slider_nib.png</textureslidernibfocus>
                        <visible>Player.Seeking</visible>
                    </control>
                </control>
			    <!-- Cutlist and Chapters -->
			    <control type="group">
				    <visible>Skin.HasSetting(ShowCutlist)</visible>
				    <control type="ranges">
			            <left>30</left>
			            <top>78</top>
			            <width>360</width>
			            <height>12</height>
			            <texturebg border="3" colordiffuse="00FFFFFF">white50.png</texturebg>
			            <lefttexture>white.png</lefttexture>
			            <midtexture colordiffuse="FFFF0000">white.png</midtexture>
			            <righttexture>white.png</righttexture>
			            <info>Player.Cutlist</info>
			        </control>
			        <control type="ranges">
    			        <left>30</left>
	    		        <top>78</top>
		    	        <width>360</width>
			            <height>6</height>
			            <texturebg border="3" colordiffuse="00FFFFFF">white50.png</texturebg>
			            <righttexture>white.png</righttexture>
			            <info>Player.Chapters</info>
			        </control>
			    </control>
			</control>	
        </control>
    </controls>
</window>
