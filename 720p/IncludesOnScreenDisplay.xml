<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="OSDMediaPlot">
		<control type="group" id="1">
			<visible>!Skin.HasSetting(EnablePauseInfo) + !IsEmpty(VideoPlayer.Plot) + Player.Paused + !Player.Caching</visible>
			<animation effect="slide" end="-140,110" time="0" tween="back" easing="out" condition="Skin.HasSetting(HideSeekBarOSD) + !Player.ShowInfo" reversible="true">Conditional</animation>
			<animation effect="slide" end="-140,0" time="0" tween="back" easing="out" condition="Skin.HasSetting(HideArtWorkOSD) + [!Skin.HasSetting(HideSeekBarOSD) | Player.ShowInfo]" reversible="true">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,25" time="0" condition="Player.ShowInfo + !VideoPlayer.Content(Movies) + !VideoPlayer.Content(Episodes) + !VideoPlayer.Content(MusicVideos) + !VideoPlayer.Content(LiveTV)">conditional</animation>
			<left>320</left>
			<top>395r</top>
			<control type="image" id="1">
				<left>0</left>
				<top>0</top>
				<width>920</width>
				<height>200</height>
				<texture border="20">ContentPanel.png</texture>
				<colordiffuse>AAFFFFFF</colordiffuse>
			</control>
			<control type="textbox" id="1">
				<description>Description Value The Movie</description>
				<left>20</left>
				<top>20</top>
				<width>880</width>
				<height>145</height>
				<align>justify</align>
				<textcolor>white</textcolor>
				<font>font13</font>
				<label>[B][COLOR=$VAR[MainColorOne]]$LOCALIZE[207]$LOCALIZE[31001] [/COLOR][/B]$VAR[VideoPlayingDescription]</label>
				<autoscroll delay="3000" time="1800" repeat="10000">!Skin.HasSetting(StopAutoScroll)</autoscroll>
			</control>
		</control>
		<control type="group" id="1">
			<visible>Player.ShowInfo + !IsEmpty(VideoPlayer.Plot) + Skin.HasSetting(EnablePlotInfo) + !Player.Caching</visible>
			<animation effect="slide" end="-140,110" time="0" tween="back" easing="out" condition="Skin.HasSetting(HideSeekBarOSD) + !Player.ShowInfo" reversible="true">Conditional</animation>
			<animation effect="slide" end="-140,0" time="0" tween="back" easing="out" condition="Skin.HasSetting(HideArtWorkOSD) + [!Skin.HasSetting(HideSeekBarOSD) | Player.ShowInfo]" reversible="true">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,25" time="0" condition="Player.ShowInfo + !VideoPlayer.Content(Movies) + !VideoPlayer.Content(Episodes) + !VideoPlayer.Content(MusicVideos) + !VideoPlayer.Content(LiveTV)">conditional</animation>
			<left>320</left>
			<top>395r</top>
			<control type="image" id="1">
				<left>0</left>
				<top>0</top>
				<width>920</width>
				<height>200</height>
				<texture border="20">ContentPanel.png</texture>
				<colordiffuse>AAFFFFFF</colordiffuse>
			</control>
			<control type="textbox" id="1">
				<description>Description Value The Movie</description>
				<left>20</left>
				<top>20</top>
				<width>880</width>
				<height>145</height>
				<align>justify</align>
				<textcolor>white</textcolor>
				<font>font13</font>
				<label>[B][COLOR=$VAR[MainColorOne]]$LOCALIZE[207]$LOCALIZE[31001] [/COLOR][/B]$VAR[VideoPlayingDescription]</label>
				<autoscroll delay="3000" time="1800" repeat="10000">!Skin.HasSetting(StopAutoScroll)</autoscroll>
			</control>
		</control>
	</include>
	<include name="OSDMovieCover">
		<control type="group" id="1">
			<control type="image" id="1">
				<description>clearart image</description>
				<left>10</left>
				<top>235r</top>
				<width>300</width>
				<height>230</height>
				<texture>$INFO[Player.Art(clearart)]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<visible>!Skin.HasSetting(DisableClearArt) + !IsEmpty(Player.Art(clearart))</visible>
			</control>
			<control type="image" id="1">
				<description>logo image</description>
				<left>10</left>
				<top>235r</top>
				<width>300</width>
				<height>230</height>
				<texture>$INFO[Player.Art(clearlogo)]</texture>
				<aspectratio aligny="bottom" align="center">keep</aspectratio>
				<visible>!Skin.HasSetting(DisableLogoArt) + !IsEmpty(Player.Art(clearlogo)) + [IsEmpty(Player.Art(clearart)) | Skin.HasSetting(DisableClearArt)]</visible>
			</control>
			<control type="image" id="1">
				<description>Cover image</description>
				<left>63</left>
				<top>339r</top>
				<width>210</width>
				<height>320</height>
				<aspectratio scalediffuse="false">stretch</aspectratio>
				<texture diffuse="ClearCase/EmptyCaseMask.png" fallback="DefaultVideo.png">$INFO[Player.Art(poster)]</texture>
				<visible>Skin.HasSetting(EnableDVDCases)</visible>
				<visible>[Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(clearart))] + [IsEmpty(Player.Art(clearlogo)) | Skin.HasSetting(DisableLogoArt)]</visible>
			</control>
			<control type="image" id="1">
				<description>Movie cover image</description>
				<left>20</left>
				<top>350r</top>
				<width>300</width>
				<height>330</height>
				<texture fallback="DefaultVideoCover.png">$INFO[Player.Art(thumb)]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<bordertexture border="8">ThumbShadow.png</bordertexture>
				<bordersize>8</bordersize>
				<visible>!Skin.HasSetting(EnableDVDCases)</visible>
				<visible>[Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(clearart))] + [IsEmpty(Player.Art(clearlogo)) | Skin.HasSetting(DisableLogoArt)]</visible>
			</control>
			<control type="image" id="1">
				<left>30</left>
				<top>350r</top>
				<width>255</width>
				<height>345</height>
				<aspectratio>stretch</aspectratio>
				<texture>$VAR[VideoPlayerCase,ClearCase/,.png]</texture>
				<visible>!Player.IsInternetStream + [Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(clearart))] + [IsEmpty(Player.Art(clearlogo)) | Skin.HasSetting(DisableLogoArt) + IsEmpty(Player.Art(thumb))]</visible>
				<visible>Skin.HasSetting(EnableDVDCases)</visible>
			</control>
			<control type="image" id="1">
				<description>clearart image</description>
				<left>10</left>
				<top>235r</top>
				<width>300</width>
				<height>230</height>
				<texture>$VAR[StreamingVideoIcon]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<visible>Player.IsInternetStream + [Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(clearart))] + [IsEmpty(Player.Art(clearlogo)) | Skin.HasSetting(DisableLogoArt) + IsEmpty(Player.Art(poster)) + IsEmpty(Player.Art(thumb))]</visible>
			</control>
			<visible>!Skin.HasSetting(HideArtWorkOSD)</visible>
		</control>
	</include>
	<include name="OSDTVShowCover">
		<control type="group" id="1">
			<control type="image" id="1">
				<description>clearart image</description>
				<left>10</left>
				<top>235r</top>
				<width>300</width>
				<height>230</height>
				<texture>$INFO[Player.Art(tvshow.clearart)]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<visible>!Skin.HasSetting(DisableClearArt) + !IsEmpty(Player.Art(tvshow.clearart))</visible>
			</control>
			<control type="image" id="1">
				<description>logo image</description>
				<left>10</left>
				<top>235r</top>
				<width>300</width>
				<height>230</height>
				<texture>$INFO[Player.Art(tvshow.clearlogo)]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<visible>!Skin.HasSetting(DisableLogoArt) + !IsEmpty(Player.Art(tvshow.clearlogo)) + [Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(tvshow.clearart))]</visible>
			</control>
			<control type="image" id="1">
				<description>Cover image</description>
				<left>63</left>
				<top>339r</top>
				<width>210</width>
				<height>320</height>
				<aspectratio scalediffuse="false">stretch</aspectratio>
				<texture diffuse="ClearCase/EmptyCaseMask.png" fallback="DefaultVideoCover.png">$INFO[Player.Art(tvshow.thumb)]</texture>
				<visible>Skin.HasSetting(EnableDVDCases)</visible>
				<visible>[Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(tvshow.clearart))] + [IsEmpty(Player.Art(tvshow.clearlogo)) | Skin.HasSetting(DisableLogoArt)] + !IsEmpty(Player.Art(tvshow.thumb))</visible>
			</control>
			<control type="image" id="1">
				<left>30</left>
				<top>350r</top>
				<width>255</width>
				<height>345</height>
				<aspectratio>stretch</aspectratio>
				<texture>$VAR[VideoPlayerCase,ClearCase/,.png]</texture>
				<visible>Skin.HasSetting(EnableDVDCases)</visible>
				<visible>[Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(tvshow.clearart))] + [IsEmpty(Player.Art(tvshow.clearlogo)) | Skin.HasSetting(DisableLogoArt)] + !IsEmpty(Player.Art(tvshow.thumb))</visible>
			</control>
			<control type="image" id="1">
				<description>cover image</description>
				<left>20</left>
				<top>235r</top>
				<width>300</width>
				<height>230</height>
				<texture fallback="DefaultVideoCover.png">$INFO[Player.Art(thumb)]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<bordertexture border="8">ThumbShadow.png</bordertexture>
				<bordersize>8</bordersize>
				<visible>[Skin.HasSetting(DisableClearArt) | IsEmpty(Player.Art(tvshow.clearart))] + [IsEmpty(Player.Art(tvshow.clearlogo)) | Skin.HasSetting(DisableLogoArt)] + [IsEmpty(Player.Art(tvshow.thumb)) | !Skin.HasSetting(EnableDVDCases)]</visible>
			</control>
			<visible>!Skin.HasSetting(HideArtWorkOSD)</visible>
		</control>
	</include>
	<include name="OSDMediaFlagging">
		<control type="grouplist" id="1">
			<left>0</left>
			<top>60</top>
			<width>910</width>
			<height>35</height>
			<itemgap>5</itemgap>
			<orientation>horizontal</orientation>
			<control type="label" id="1">
				<width min="10" max="520">auto</width>
				<height>30</height>
				<font>font30</font>
				<align>left</align>
				<aligny>center</aligny>
				<label>$VAR[VideoNowPlaying]</label>
				<textcolor>orange</textcolor>
				<scroll>true</scroll>
			</control>
			<control type="image" id="1">
				<description>Video Resolution Image</description>
				<width>50</width>
				<height>35</height>
				<aspectratio align="center">keep</aspectratio>
				<texture>$INFO[VideoPlayer.VideoResolution,flagging/video/,.png]</texture>
			</control>
			<control type="image" id="1">
				<description>Audio Codec Image</description>
				<width>85</width>
				<height>35</height>
				<aspectratio align="center">keep</aspectratio>
				<texture>$INFO[VideoPlayer.AudioCodec,flagging/audio/,.png]</texture>
			</control>
			<control type="image" id="1">
				<description>Audio Channels Image</description>
				<width>50</width>
				<height>35</height>
				<aspectratio align="center">keep</aspectratio>
				<texture>$INFO[VideoPlayer.AudioChannels,flagging/audio/,.png]</texture>
			</control>
			<control type="group" id="1">
				<width>85</width>
				<height>35</height>
				<visible>!IsEmpty(VideoPlayer.AudioLanguage)</visible>
				<control type="image" id="1">
					<left>5</left>
					<top>0</top>
					<description>Audio Language Image</description>
					<width>80</width>
					<height>35</height>
					<texture>flagging/AudioLang.png</texture>
				</control>
				<control type="label" id="1">
					<left>38</left>
					<top>2</top>
					<width>47</width>
					<height>35</height>
					<font>font13</font>
					<align>left</align>
					<label>$INFO[VideoPlayer.AudioLanguage]</label>
					<textcolor>grey</textcolor>
				</control>
			</control>
			<control type="group" id="1">
				<width>85</width>
				<height>35</height>
				<visible>!IsEmpty(VideoPlayer.SubtitlesLanguage)</visible>
				<control type="image" id="1">
					<left>5</left>
					<top>0</top>
					<width>80</width>
					<height>35</height>
					<texture>flagging/SubLang.png</texture>
				</control>
				<control type="label" id="1">
					<left>38</left>
					<top>2</top>
					<width>47</width>
					<height>35</height>
					<font>font13</font>
					<align>left</align>
					<label>$INFO[VideoPlayer.SubtitlesLanguage]</label>
					<textcolor>grey</textcolor>
				</control>
			</control>
		</control>
	</include>
	<include name="OSDMediaHeading">
		<control type="label" id="1">
			<description>Now Playing Heading label</description>
			<left>0</left>
			<top>0</top>
			<width>910</width>
			<height>25</height>
			<align>left</align>
			<aligny>center</aligny>
			<font>font13</font>
			<label>$LOCALIZE[31040] - $LOCALIZE[21396]$LOCALIZE[31001] $INFO[player.chapter]$INFO[player.chaptercount, $LOCALIZE[31088] ]</label>
			<textcolor>white</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>Player.ChapterCount + !VideoPlayer.Content(LiveTV)</visible>
			<animation effect="slide" start="0,0" end="0,25" time="0" condition="!VideoPlayer.Content(Movies) + !VideoPlayer.Content(Episodes) + !VideoPlayer.Content(MusicVideos) + !VideoPlayer.Content(LiveTV)">conditional</animation>
		</control>
		<control type="label" id="1">
			<description>Now Playing LiveTV Heading label</description>
			<left>0</left>
			<top>0</top>
			<width>910</width>
			<height>25</height>
			<align>left</align>
			<aligny>center</aligny>
			<font>font13</font>
			<label>$LOCALIZE[31040] - $INFO[VideoPlayer.ChannelGroup,$LOCALIZE[31509]$LOCALIZE[31001] ]</label>
			<textcolor>white</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>VideoPlayer.Content(LiveTV) + VideoPlayer.ChannelGroup + !Player.Recording</visible>
		</control>
		<control type="image" id="1">
			<top>0</top>
			<width>50</width>
			<height>25</height>
			<aspectratio align="center" aligny="center">keep</aspectratio>
			<texture>PVR-IsRecording.png</texture>
			<visible>VideoPlayer.Content(LiveTV) + Player.Recording</visible>
		</control>
		<control type="label" id="1">
			<description>Heading label</description>
			<left>50</left>
			<top>0</top>
			<width>860</width>
			<height>25</height>
			<align>left</align>
			<aligny>center</aligny>
			<font>font13</font>
			<label>$LOCALIZE[19158]</label>
			<textcolor>white</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>VideoPlayer.Content(LiveTV) + Player.Recording</visible>
		</control>
		<control type="label" id="1">
			<description>Studio label</description>
			<left>0</left>
			<top>30</top>
			<width>930</width>
			<height>25</height>
			<align>left</align>
			<font>font12</font>
			<label>$INFO[VideoPlayer.Studio]</label>
			<textcolor>grey2</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>VideoPlayer.Content(Movies)</visible>
		</control>
		<control type="label" id="1">
			<description>TV Show label</description>
			<left>0</left>
			<top>30</top>
			<width>930</width>
			<height>25</height>
			<align>left</align>
			<font>font12</font>
			<label>$INFO[VideoPlayer.TVShowTitle] ($INFO[VideoPlayer.Season,,x]$INFO[VideoPlayer.Episode])</label>
			<textcolor>grey2</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>VideoPlayer.Content(Episodes)</visible>
		</control>
		<control type="label" id="1">
			<description>Music Info label</description>
			<left>0</left>
			<top>30</top>
			<width>930</width>
			<height>25</height>
			<align>left</align>
			<font>font12</font>
			<label>$INFO[VideoPlayer.Artist] $INFO[VideoPlayer.Album, - ]</label>
			<textcolor>grey2</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>VideoPlayer.Content(MusicVideos)</visible>
		</control>
		<control type="label" id="1">
			<description>LiveTV Info label</description>
			<left>0</left>
			<top>30</top>
			<width>930</width>
			<height>25</height>
			<align>left</align>
			<font>font12</font>
			<label>([COLOR=$VAR[MainColorOne]],[/COLOR]) $INFO[VideoPlayer.ChannelNumberLabel]$INFO[VideoPlayer.ChannelName]$INFO[VideoPlayer.EpisodeName, (,)]</label>
			<textcolor>grey2</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>VideoPlayer.Content(LiveTV)</visible>
		</control>
	</include>
	<include name="MusicArtUnderVisualisation">
		<control type="group">
			<visible>Skin.HasSetting(NoDiffuseVizFanart) | stringcompare(Visualisation.Name,Waveform) | stringcompare(Visualisation.Name,DirectX Spectrum) | stringcompare(Visualisation.Name,OpenGL Spectrum)</visible>
			<visible>!Skin.HasSetting(HideHomeNowPlaying)</visible>
			<control type="multiimage">
				<include>Dimensions_Fullscreen</include>
				<include>BackgroundTimePerImage</include>
				<randomize>true</randomize>
				<fadetime>600</fadetime>
				<aspectratio>keep</aspectratio>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
				<imagepath background="true">$INFO[Player.FolderPath,,../extrafanart/]</imagepath>
				<visible>Skin.HasSetting(DisableSlideShowArt) + !IsEmpty(Control.GetLabel(5043))</visible>
			</control>
			<control type="multiimage">
				<include>Dimensions_Fullscreen</include>
				<include>BackgroundTimePerImage</include>
				<randomize>true</randomize>
				<fadetime>600</fadetime>
				<aspectratio>keep</aspectratio>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
				<imagepath background="true">$INFO[Skin.String(PhysicalMusicFolder)]$INFO[MusicPlayer.Artist,,/extrafanart/]</imagepath>
				<visible>Skin.HasSetting(DisableSlideShowArt) + IsEmpty(Control.GetLabel(5043))</visible>
			</control>
			<control type="image">
				<description>Fanart Image for Artist</description>
				<include>Dimensions_Fullscreen</include>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
				<texture background="true">$INFO[Player.Art(fanart)]</texture>
				<visible>[!IsEmpty(Player.Art(fanart)) + IsEmpty(Control.GetLabel(5043)) + IsEmpty(Control.GetLabel(5044))] | [!Skin.HasSetting(DisableSlideShowArt) + IsEmpty(Window(Visualisation).Property(ArtistSlideshow.ArtworkReady))]</visible>
				<fadetime>600</fadetime>
			</control>
			<control type="multiimage">
				<include>Dimensions_Fullscreen</include>
				<include>BackgroundTimePerImage</include>
				<randomize>true</randomize>
				<fadetime>600</fadetime>
				<aspectratio>keep</aspectratio>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
				<imagepath background="true">$INFO[Window(Visualisation).Property(ArtistSlideshow)]</imagepath>
				<visible>!Skin.HasSetting(DisableSlideShowArt) + !IsEmpty(Window(Visualisation).Property(ArtistSlideshow.ArtworkReady))</visible>
			</control>
		</control>
	</include>
	<include name="MusicArtOverVisualisation">
		<control type="group">
			<visible>!Skin.HasSetting(HideHomeNowPlaying) + !Skin.HasSetting(NoDiffuseVizFanart)</visible>
			<visible>!stringcompare(Visualisation.Name,Waveform) + !stringcompare(Visualisation.Name,DirectX Spectrum) + !stringcompare(Visualisation.Name,OpenGL Spectrum)</visible>
			<control type="multiimage">
				<include>Dimensions_Fullscreen</include>
				<include>BackgroundTimePerImage</include>
				<randomize>true</randomize>
				<fadetime>600</fadetime>
				<aspectratio>keep</aspectratio>
				<colordiffuse>AAFFFFFF</colordiffuse>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
				<imagepath background="true">$INFO[Player.FolderPath,,../extrafanart/]</imagepath>
				<visible>!IsEmpty(Control.GetLabel(5043))</visible>
				<visible>Skin.HasSetting(DisableSlideShowArt) | [!Skin.HasSetting(DisableSlideShowArt) + !IsEmpty(Window(Visualisation).Property(ArtistSlideshowTransparent)) + IsEmpty(Window(Visualisation).Property(ArtistSlideshow.ArtworkReady))]</visible>
			</control>
			<control type="multiimage">
				<include>Dimensions_Fullscreen</include>
				<include>BackgroundTimePerImage</include>
				<randomize>true</randomize>
				<fadetime>600</fadetime>
				<aspectratio>keep</aspectratio>
				<colordiffuse>AAFFFFFF</colordiffuse>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
				<imagepath background="true">$INFO[Skin.String(PhysicalMusicFolder)]$INFO[MusicPlayer.Artist,,/extrafanart/]</imagepath>
				<visible>IsEmpty(Control.GetLabel(5043))</visible>
				<visible>Skin.HasSetting(DisableSlideShowArt) | [!Skin.HasSetting(DisableSlideShowArt) + !IsEmpty(Window(Visualisation).Property(ArtistSlideshowTransparent)) + IsEmpty(Window(Visualisation).Property(ArtistSlideshow.ArtworkReady))]</visible>
			</control>
			<control type="image">
				<description>Fanart Image for Artist</description>
				<include>Dimensions_Fullscreen</include>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
				<texture background="true">$INFO[Player.Art(fanart)]</texture>
				<colordiffuse>AAFFFFFF</colordiffuse>
				<fadetime>600</fadetime>
				<visible>Skin.HasSetting(DisableSlideShowArt) | [!Skin.HasSetting(DisableSlideShowArt) + !IsEmpty(Window(Visualisation).Property(ArtistSlideshowTransparent)) + IsEmpty(Window(Visualisation).Property(ArtistSlideshow.ArtworkReady))]</visible>
				<visible>!IsEmpty(Player.Art(fanart)) + IsEmpty(Control.GetLabel(5043)) + IsEmpty(Control.GetLabel(5044))</visible>
			</control>
			<control type="multiimage">
				<include>Dimensions_Fullscreen</include>
				<include>BackgroundTimePerImage</include>
				<randomize>true</randomize>
				<fadetime>600</fadetime>
				<aspectratio>keep</aspectratio>
				<include condition="!Skin.HasSetting(NoAnimateVizFanart)">KBurnsAnimateFanart</include>
