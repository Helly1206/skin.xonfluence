<?xml version="1.0" encoding="utf-8"?>
<includes>
	<include name="WrapHomeMainMenu">
		<control type="wraplist" id="9000">
			<include>HomeMainMenuSys</include>
			<include>HomeMainMenuContent</include>
			<visible>Skin.HasSetting(WrapAroundHomeMenu)</visible>
		</control>
	</include>
	<include name="FixedHomeMainMenu">
		<control type="fixedlist" id="9000">
			<include>HomeMainMenuSys</include>
			<include>HomeMainMenuContent</include>
			<visible>!Skin.HasSetting(WrapAroundHomeMenu)</visible>
		</control>
	</include>
	<include name="HomeMainMenuSys">
		<left>-110</left>
		<top>5</top>
		<width>1500</width>
		<height>60</height>
		<onleft condition="!Skin.HasSetting(FixedEndingMenu)">9000</onleft>
		<onright condition="!Skin.HasSetting(FixedEndingMenu)">9000</onright>
		<onleft condition="Skin.HasSetting(FixedEndingMenu)">noop</onleft>
		<onright condition="Skin.HasSetting(FixedEndingMenu)">noop</onright>
		<onup condition="!Skin.HasSetting(DisableGlobalSearch)">610</onup>
		<onup condition="Skin.HasSetting(DisableGlobalSearch)">603</onup>
		<ondown>9001</ondown>
		<scrolltime>300</scrolltime>
		<focusposition>2</focusposition>
		<movement>1</movement>
		<orientation>Horizontal</orientation>
		<itemlayout height="60" width="300">
			<control type="image">
				<left>0</left>
				<top>4</top>
				<width>1</width>
				<height>52</height>
				<texture>HomeSeperator.png</texture>
			</control>
			<control type="image">
				<left>299</left>
				<top>4</top>
				<width>1</width>
				<height>52</height>
				<texture>HomeSeperator.png</texture>
			</control>
			<control type="label">
				<left>5</left>
				<top>0</top>
				<width>290</width>
				<height>60</height>
				<font>font_MainMenu</font>
				<textcolor>grey3</textcolor>
				<align>center</align>
				<aligny>center</aligny>
				<label>$INFO[ListItem.Label]</label>
			</control>
		</itemlayout>
		<focusedlayout height="60" width="300">
			<control type="image">
				<left>0</left>
				<top>4</top>
				<width>1</width>
				<height>52</height>
				<texture>HomeSeperator.png</texture>
			</control>
			<control type="image">
				<left>299</left>
				<top>4</top>
				<width>1</width>
				<height>52</height>
				<texture>HomeSeperator.png</texture>
			</control>
			<control type="label">
