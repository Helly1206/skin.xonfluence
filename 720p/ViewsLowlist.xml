<?xml version="1.0" encoding="utf-8"?>
<includes>
	<include name="Lowlist">
		<control type="group">
			<visible>Control.IsVisible(724)</visible>
			<include>VisibleFadeEffect</include>
			<control type="list" id="724">
				<left>340</left>
				<top>440</top>
				<width>580</width>
				<height>160</height>
				<onleft>9000</onleft>
				<onright>60</onright>
				<onup>724</onup>
				<ondown>724</ondown>
				<viewtype label="$LOCALIZE[31648]">list</viewtype>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<visible>Container.Content(Movies) | Container.Content(Seasons) | Container.Content(TVShows) | Container.Content(MusicVideos)</visible>
				<visible>!Container.Content(LiveTV)</visible>
				<itemlayout height="40" width="480">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>480</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemNF.png</texture>
					</control>
					<control type="label">
						<left>10</left>
						<top>0</top>
						<width>460</width>
						<height>40</height>
						<font>font13</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>430</left>
						<top>0</top>
						<width>400</width>
						<height>40</height>
						<font>font12</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
					<control type="image">
						<left>430</left>
						<top>14</top>
						<width>20</width>
						<height>16</height>
						<texture>$INFO[ListItem.Overlay]</texture>
						<aspectratio>keep</aspectratio>
					</control>
				</itemlayout>
				<focusedlayout height="40" width="480">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>480</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemNF.png</texture>
						<visible>!Control.HasFocus(724)</visible>
						<include>VisibleFadeEffect</include>
					</control>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>460</width>
						<height>41</height>
						<texture border="0,2,0,2" colordiffuse="$VAR[MainColorOne]">MenuItemFO.png</texture>
						<visible>Control.HasFocus(724)</visible>
						<include>VisibleFadeEffect</include>
					</control>
					<control type="image">
						<left>370</left>
						<top>5</top>
						<width>100</width>
						<height>31</height>
						<texture border="0,0,14,0" colordiffuse="$VAR[MainColorOne]">MediaItemDetailBG.png</texture>
						<visible>Control.HasFocus(724)</visible>
					</control>
					<control type="label">
						<left>10</left>
						<top>0</top>
						<width>460</width>
						<height>40</height>
						<font>font13</font>
						<textcolor>white</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>430</left>
						<top>0</top>
						<width>400</width>
						<height>40</height>
						<font>font12</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
					<control type="image">
						<left>430</left>
						<top>14</top>
						<width>20</width>
						<height>16</height>
						<texture>$INFO[ListItem.Overlay]</texture>
						<aspectratio>keep</aspectratio>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>810</left>
				<top>440</top>
				<width>25</width>
				<height>160</height>
				<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,14,0,14" colordiffuse="$VAR[MainColorOne]">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernibfocus>
				<onleft>724</onleft>
				<onright>800</onright>
				<showonepage>true</showonepage>
				<orientation>vertical</orientation>
				<visible>Control.IsVisible(724)</visible>
			</control>
			<control type="group">
				<visible>Control.IsVisible(724)</visible>
				<control type="group">
					<visible>IsEmpty(ListItem.Art(poster)) | !Window.IsVisible(Videos)</visible>
					<left>40</left>
					<top>185</top>
					<camera x="640" y="200" />
					<animation effect="rotatey" center="50" start="-30" end="-30" time="0" condition="true">Conditional</animation>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>260</width>
						<height>480</height>
						<aspectratio aligny="bottom">keep</aspectratio>
						<fadetime>IconCrossfadeTime</fadetime>
						<texture background="true" diffuse="ClearCase/EmptyCaseMask.png">$INFO[ListItem.thumbnailImage]</texture>
						<visible>!IsEmpty(ListItem.thumbnailImage)</visible>
					</control>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>260</width>
						<height>480</height>
						<aspectratio aligny="bottom">keep</aspectratio>
						<fadetime>IconCrossfadeTime</fadetime>
						<texture background="true" diffuse="ClearCase/EmptyCaseMask.png">$INFO[ListItem.Icon]</texture>
						<visible>IsEmpty(ListItem.thumbnailImage)</visible>
					</control>
				</control>
				<control type="group">
					<visible>!IsEmpty(ListItem.Art(poster)) + Window.IsVisible(Videos)</visible>
					<left>40</left>
					<top>185</top>
					<camera x="640" y="200" />
					<animation effect="rotatey" center="50" start="-30" end="-30" time="0" condition="true">Conditional</animation>
					<control type="image">
						<left>0</left>
						<top>-15</top>
						<width>260</width>
						<height>480</height>
						<aspectratio>stretch</aspectratio>
						<fadetime>IconCrossfadeTime</fadetime>
						<texture background="true" diffuse="ClearCase/EmptyCaseMask.png">$VAR[PosterThumb]</texture>
						<visible>!Skin.HasSetting(EnableDVDCases)</visible>
					</control>
					<control type="image">
						<left>33</left>
						<top>15</top>
						<width>217</width>
						<height>445</height>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<fadetime>IconCrossfadeTime</fadetime>
						<texture background="true" diffuse="ClearCase/EmptyCaseMask.png">$VAR[PosterThumb]</texture>
						<visible>Skin.HasSetting(EnableDVDCases)</visible>
					</control>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>260</width>
						<height>480</height>
						<aspectratio>stretch</aspectratio>
						<texture fallback="ClearCase/dv.png">$VAR[VideoListCase,ClearCase/,.png]</texture>
						<visible>Skin.HasSetting(EnableDVDCases)</visible>
					</control>
				</control>
				<control type="grouplist">
					<description>Media Codec Flagging Images</description>
					<left>260</left>
					<top>105r</top>
					<width>570</width>
					<align>center</align>
					<itemgap>2</itemgap>
					<orientation>horizontal</orientation>
					<include>VideoMPAAFlaggingConditions</include>
					<include>VideoCodecFlaggingConditions</include>
					<include>AudioCodecFlaggingConditions</include>
					<include>AudioChannelsFlaggingConditions</include>
					<include>AspectCodecFlaggingConditions</include>
					<include>VideoExtrasFlaggingConditions</include>
					<include>VideoTypeHackFlaggingConditions</include>
				</control>
				<control type="textbox">
					<description>Description Value for Movie</description>
					<left>840</left>
					<top>440</top>
					<width>420</width>
					<height>160</height>
					<font>font12</font>
					<align>justify</align>
					<textcolor>white</textcolor>
					<label>$VAR[VideoDescription,[B]$LOCALIZE[207]: [/B]]</label>
					<autoscroll time="2600" delay="6000" repeat="6000">!Skin.HasSetting(StopAutoScroll)</autoscroll>
					<visible>!StringCompare(ListItem.Label,..)</visible>
				</control>
				<control type="image">
					<description>Rating value</description>
					<left>840</left>
					<top>620</top>
					<width>150</width>
					<height>25</height>
					<aspectratio align="left">keep</aspectratio>
					<texture>LeftRating/$INFO[ListItem.StarRating]</texture>
					<visible>!IsEmpty(ListItem.StarRating)</visible>
				</control>
				<control type="label">
					<right>30</right>
					<top>620</top>
					<width>300</width>
					<height>30</height>
					<font>font12</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
					<scroll>false</scroll>
					<align>right</align>
					<aligny>top</aligny>
					<label>$VAR[MediaRuntime,[B]$LOCALIZE[2050]: [/B]]</label>
					<visible>!IsEmpty(ListItem.Duration)</visible>
				</control>
			</control>
		</control>
	</include>
</includes>
