				<texturenofocus border="5">button-nofocus.png</texturenofocus>
				<texturefocus border="5" colordiffuse="$VAR[MainColorOne]">button-focus.png</texturefocus>
				<label>186</label>
				<font>font12_title</font>
				<onup>13</onup>
				<onleft>2</onleft>
				<onright>2</onright>
				<ondown>10</ondown>
