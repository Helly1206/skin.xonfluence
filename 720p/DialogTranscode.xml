			<description>Resolution</description>
			<left>40</left>
			<top>200</top>
			<width>720</width>
			<height>40</height>
			<font>font13</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturenofocus border="5">button-nofocus.png</texturenofocus>
