<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="PictureWrapView">
		<control type="group">
			<visible>Control.IsVisible(510)</visible>
			<include>VisibleFadeEffect</include>
			<left>0</left>
			<top>440</top>
			<control type="wraplist" id="510">
				<depth>DepthContent+</depth>
				<left>-25</left>
				<top>5</top>
				<width>1330</width>
				<height>250</height>
				<onleft>510</onleft>
				<onright>510</onright>
				<onup>2</onup>
				<ondown>60</ondown>
				<viewtype label="31032">wrap</viewtype>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<focusposition>3</focusposition>
				<orientation>horizontal</orientation>
				<preloaditems>4</preloaditems>
				<itemlayout height="200" width="190">
					<control type="image">
						<left>15</left>
						<top>40</top>
						<width>160</width>
						<height>160</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="10">ThumbShadow.png</bordertexture>
						<bordersize>8</bordersize>
						<fadetime>200</fadetime>
						<texture border="2">ThumbBG.png</texture>
					</control>
					<control type="image">
						<left>25</left>
						<top>50</top>
						<width>140</width>
						<height>140</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="200" width="190">
					<control type="image">
						<depth>DepthContentPopout</depth>
						<left>15</left>
						<top>40</top>
						<width>160</width>
						<height>160</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="10">ThumbShadow.png</bordertexture>
						<bordersize>8</bordersize>
						<fadetime>200</fadetime>
						<texture border="2">ThumbBG.png</texture>
						<animation reversible="false" effect="zoom" start="15,40,160,160" end="-10,-5,210,210" time="150">focus</animation>
						<animation reversible="false" effect="zoom" end="15,40,160,160" start="-10,-5,210,210" time="150">unfocus</animation>
					</control>
					<control type="image">
						<depth>DepthContentPopout</depth>
						<left>25</left>
						<top>50</top>
						<width>140</width>
						<height>140</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
						<animation reversible="false" effect="zoom" start="25,50,140,140" end="5,10,180,180" time="150">focus</animation>
						<animation reversible="false" effect="zoom" end="25,50,140,140" start="5,10,180,180" time="150">unfocus</animation>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>310</left>
				<top>200</top>
				<width>660</width>
				<height>25</height>
				<texturesliderbackground border="14,0,14,0">ScrollBarH.png</texturesliderbackground>
				<texturesliderbar border="14,0,14,0">ScrollBarH_bar.png</texturesliderbar>
				<texturesliderbarfocus border="14,0,14,0" colordiffuse="$VAR[MainColorOne]">ScrollBarH_bar_focus.png</texturesliderbarfocus>
				<textureslidernib colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernibfocus>
				<onup>510</onup>
				<ondown>803</ondown>
				<showonepage>true</showonepage>
				<orientation>horizontal</orientation>
				<visible>Control.IsVisible(510)</visible>
			</control>
		</control>
	</include>
	<include name="PictureThumbView">
		<control type="group">
			<visible>Control.IsVisible(514)</visible>
			<control type="panel" id="514">
				<left>60</left>
				<top>75</top>
				<width>432</width>
				<height>576</height>
				<onleft>2</onleft>
				<onright>60</onright>
				<onup>514</onup>
				<ondown>514</ondown>
				<viewtype label="$LOCALIZE[31031]">list</viewtype>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<preloaditems>2</preloaditems>
				<itemlayout height="144" width="144">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>144</width>
						<height>144</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="10">ThumbShadow.png</bordertexture>
						<bordersize>8</bordersize>
						<fadetime>200</fadetime>
						<texture border="2">ThumbBG.png</texture>
					</control>
					<control type="image">
						<left>10</left>
						<top>10</top>
						<width>124</width>
						<height>124</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="144" width="144">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>144</width>
						<height>144</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="10" colordiffuse="$VAR[MainColorOne]">folder-focus.png</bordertexture>
						<bordersize>8</bordersize>
						<fadetime>200</fadetime>
						<texture border="2">ThumbBG.png</texture>
					</control>
					<control type="image">
						<left>10</left>
						<top>10</top>
						<width>124</width>
						<height>124</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>500</left>
				<top>70</top>
				<width>25</width>
				<height>576</height>
				<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,14,0,14" colordiffuse="$VAR[MainColorOne]">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernibfocus>
				<onleft>514</onleft>
				<onright>800</onright>
				<showonepage>true</showonepage>
				<orientation>vertical</orientation>
				<visible>Control.IsVisible(514)</visible>
			</control>
			<control type="group">
				<left>570</left>
				<top>80</top>
				<control type="label">
					<description>Title txt</description>
					<left>0</left>
					<top>0</top>
					<width>640</width>
					<height>15</height>
					<label>$INFO[ListItem.Label]</label>
					<align>center</align>
					<aligny>center</aligny>
					<font>font24_title</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
				</control>
				<control type="image">
					<left>0</left>
					<top>30</top>
					<width>640</width>
					<height>470</height>
					<texture background="true">$INFO[ListItem.FilenameAndPath]</texture>
					<fadetime>600</fadetime>
					<aspectratio>keep</aspectratio>
					<bordertexture border="8">ThumbShadow.png</bordertexture>
					<bordersize>9</bordersize>
				</control>
				<control type="label">
					<description>Date time txt</description>
					<left>0</left>
					<top>510</top>
					<width>640</width>
					<height>15</height>
					<label>[COLOR=$VAR[MainColorOne]]$LOCALIZE[31326][/COLOR] - $INFO[ListItem.PictureDateTime]</label>
					<align>left</align>
					<aligny>center</aligny>
					<font>font13</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
					<visible>!String.IsEmpty(listItem.PictureDatetime)</visible>
				</control>
				<control type="label">
					<description>Resolution txt</description>
					<left>0</left>
					<top>535</top>
					<width>640</width>
					<height>15</height>
					<label>[COLOR=$VAR[MainColorOne]]$LOCALIZE[31999][/COLOR] - $INFO[ListItem.PictureResolution]</label>
					<align>left</align>
					<aligny>center</aligny>
					<font>font13</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
					<visible>!String.IsEmpty(listItem.PictureResolution)</visible>
				</control>
			</control>
		</control>
	</include>
	<include name="PictureFullWallView">
		<control type="group">
			<visible>Control.IsVisible(532)</visible>
			<control type="image">
				<include>Dimensions_Fullscreen</include>
				<texture background="true">black-back.png</texture>
			</control>
			<control type="panel" id="532">
				<hitrect x="0" y="-55" w="1280" h="720" />
				<left>29</left>
				<top>55</top>
				<width>1251</width>
				<height>606</height>
				<onleft>732</onleft>
				<onright>732</onright>
				<onup>2</onup>
				<ondown>2</ondown>
				<viewtype label="31030">panel</viewtype>
				<pagecontrol>-</pagecontrol>
				<scrolltime>200</scrolltime>
				<preloaditems>200</preloaditems>
				<itemlayout height="202" width="139">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>139</width>
						<height>202</height>
						<include>TextureBorderViews</include>
						<bordertexture border="5">button-nofocus.png</bordertexture>
						<aspectratio scalediffuse="false">scale</aspectratio>
						<include>VisibleFadeEffect</include>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>139</width>
						<height>202</height>
						<include>TextureBorderViews</include>
						<bordertexture border="5">button-nofocus.png</bordertexture>
						<aspectratio>stretch</aspectratio>
						<texture>black-back.png</texture>
						<colordiffuse>AAFFFFFF</colordiffuse>
						<visible>!String.IsEqual(ListItem.Label,..)</visible>
					</control>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>139</width>
						<height>202</height>
						<include>TextureBorderViews</include>
						<bordertexture border="5">button-nofocus.png</bordertexture>
						<aspectratio>stretch</aspectratio>
						<texture>black-back.png</texture>
						<colordiffuse>AAFFFFFF</colordiffuse>
						<include condition="!String.IsEmpty(Skin.String(MediaPanelTrans))">TransparentEffect</include>
						<visible>!String.IsEqual(ListItem.Label,..)</visible>
					</control>
				</itemlayout>
				<focusedlayout height="205" width="142">
					<control type="group">
						<left>0</left>
						<top>0</top>
						<animation effect="slide" end="22,0" time="200" reversible="true" condition="Container.Column(0)">conditional</animation>
						<animation effect="slide" end="0,26" time="200" reversible="true" condition="Container.Row(0)">conditional</animation>
						<animation effect="slide" end="0,-26" time="200" reversible="true" condition="Container.Row(2)">conditional</animation>
						<animation effect="slide" end="-22,0" time="200" reversible="true" condition="Container.Column(8)">conditional</animation>
						<animation reversible="false" effect="zoom" end="-27,-33,185,267" time="100">focus</animation>
						<animation reversible="false" effect="zoom" start="-27,-33,185,267" time="100">unfocus</animation>
						<control type="image">
							<left>0</left>
							<top>0</top>
							<width>142</width>
							<height>205</height>
							<aspectratio scalediffuse="false">scale</aspectratio>
							<include>VisibleFadeEffect</include>
							<bordersize>8</bordersize>
							<texture background="true">$INFO[ListItem.Icon]</texture>
							<bordertexture border="8">ThumbBorder.png</bordertexture>
						</control>
					</control>
				</focusedlayout>
			</control>
		</control>
	</include>
</includes>
