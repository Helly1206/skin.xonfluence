<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>1900&apos;s</name>
    <match>all</match>
    <rule field="year" operator="greaterthan">
        <value>1900</value>
    </rule>
    <rule field="year" operator="lessthan">
        <value>1910</value>
    </rule>
    <order direction="ascending">year</order>
</smartplaylist>
