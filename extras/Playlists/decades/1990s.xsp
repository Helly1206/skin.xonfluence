<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>1990&apos;s</name>
    <match>all</match>
    <rule field="year" operator="greaterthan">
        <value>1989</value>
    </rule>
    <rule field="year" operator="lessthan">
        <value>2000</value>
    </rule>
    <order direction="ascending">year</order>
</smartplaylist>
